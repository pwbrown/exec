{
    "compilerOptions": {
        "module": "commonjs",
        "moduleResolution": "node",
        "target": "es2015",
        "skipLibCheck": true,
        "esModuleInterop": true,
        "strict": true,
        "jsx": "react",
        "baseUrl": ".",
        "paths": {
            "store/*": ["store/*"],
            "types/*": ["types/*"],
            "utils/*": ["utils/*"],
        }
    },
    "include": [ "./**/*" ]
}