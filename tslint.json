{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react",
        "tslint-eslint-rules",
        "tslint-react-hooks"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single"],
        "react-hooks-nesting": "error",
        "variable-name": {
            "options": [
                "check-format",
                "allow-pascal-case",
                "allow-leading-underscore"
            ]
        }
    },
    "rulesDirectory": []
}